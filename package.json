{
  "name": "upr-dashboard",
  "version": "1.0.0",
  "description": "UAE Premium Radar Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "pm2-runtime start ecosystem.config.cjs",
    "dev": "vite",
    "build": "vite build --logLevel debug",
    "preview": "vite preview --port 5173",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:siva": "jest server/siva-tools/__tests__",
    "checkpoint": "node scripts/generate_checkpoint.mjs",
    "snapshot:bundle": "node scripts/bundle_snapshot.mjs",
    "rebuild": "echo 'Forcing a clean build cache on deploy'",
    "sprint:sync": "cd scripts/notion && npm run sprint:sync",
    "sprint:pull": "cd scripts/notion && npm run sprint:pull",
    "sprint:close": "cd scripts/notion && npm run sprint:close",
    "sprint:template": "cd scripts/notion && npm run sprint:template",
    "notion": "cd scripts/notion && npm run notion --",
    "notion:setup": "cd scripts/notion && npm run create-roadmap",
    "notion:populate": "cd scripts/notion && npm run populate-upr-data",
    "notion:enhance": "cd scripts/notion && npm run enhance",
    "notion:metrics": "cd scripts/notion && npm run metrics",
    "notion:sync-siva": "node scripts/notion/syncSIVAProgress.js",
    "notion:sync-siva-docs": "node scripts/notion/syncSIVAProgressWithDocs.js",
    "notion:sync-all": "node scripts/notion/syncAll.js",
    "sprint:create-dbs": "node scripts/notion/createSprintDatabases.js",
    "sprint:populate": "node scripts/notion/populateSprints.js",
    "sprint:create-tool": "node scripts/notion/createSIVATool.js",
    "sprint:create-work": "node scripts/notion/createWorkItem.js",
    "docs:sync": "node scripts/notion/syncMarkdownDocs.js",
    "knowledge:create": "node scripts/notion/createKnowledgeBase.js",
    "deploy": "bash scripts/deploy.sh",
    "backup": "bash scripts/backup-db.sh",
    "health": "bash scripts/health-check.sh",
    "costs": "bash scripts/check-gcp-costs.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@notionhq/client": "^2.2.15",
    "@sentry/node": "^10.23.0",
    "@sentry/react": "^10.23.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.23.22",
    "ioredis": "^5.8.1",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "mathjs": "^15.1.0",
    "multer": "^2.0.2",
    "neo4j-driver": "^5.28.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "openai": "^6.2.0",
    "pdf-parse": "^2.3.12",
    "pg": "^8.16.3",
    "pm2": "^6.0.13",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "string-similarity": "^4.0.4",
    "winston": "^3.18.3"
