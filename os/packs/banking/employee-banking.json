      },
      {
        "name": "Union National Bank",
        "aliases": ["UNB"],
        "domains": ["unb.com"]
      }
    ]
  },

  "progressive_delivery": {
    "description": "Config for progressive lead delivery - show leads in batches, not all at once",
    "enabled": true,
    "initial_batch": 5,
    "subsequent_batch": 3,
    "require_feedback_before_next": true,
    "max_leads_per_session": 50
  },

  "preference_learning": {
    "description": "Config for OS preference learning system",
    "enabled": true,
    "signals_to_track": ["LIKE", "DISLIKE", "SAVE", "DISMISS", "SHOW_MORE", "FIND_SIMILAR"],
    "learning_window_days": 90,
    "min_signals_for_learning": 5,
    "preference_factors": ["industry", "company_size", "location", "signal_type", "founding_year"]
  }
}
